    <feature name="NetworkStatus">
        <param name="ios-package" value="CDVConnection" />
    </feature>
    <feature name="SplashScreen">
        <param name="ios-package" value="CDVSplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <feature name="StatusBar">
        <param name="ios-package" onload="true" value="CDVStatusBar" />
    </feature>
    <preference name="StatusBarBackgroundColor" value="#000000" />
    <feature name="UniversalAnalytics">
        <param name="ios-package" value="UniversalAnalyticsPlugin" />
    </feature>
    <feature name="Clipboard">
        <param name="ios-package" value="CDVClipboard" />
    </feature>
    <feature name="Sms">
        <param name="ios-package" value="Sms" />
    </feature>
    <name>Facing</name>
    <description>
        GPS &amp; Compass Frameworks to Connect People to Each Other http://youfacing.me
    </description>
    <author email="hello@manifestinteractive.com" href="http://manifestinteractive.com">
        Manifest Interactive
    </author>
    <preference name="phonegap-version" value="3.5.0" />
    <preference name="Orientation" value="portrait" />
    <preference name="Fullscreen" value="true" />
    <preference name="webviewbounce" value="false" />
    <preference name="prerendered-icon" value="true" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="BackgroundColor" value="0xff222222" />
    <preference name="HideKeyboardFormAccessoryBar" value="true" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <gap:config-file parent="UIStatusBarHidden" platform="ios">
        <true />
    </gap:config-file>
    <gap:config-file parent="UIViewControllerBasedStatusBarAppearance" platform="ios">
        <false />
    </gap:config-file>
    <gap:plugin name="org.apache.cordova.console" version="0.2.7" />
    <gap:plugin name="org.apache.cordova.contacts" version="0.2.11" />
    <gap:plugin name="org.apache.cordova.device" version="0.2.8" />
